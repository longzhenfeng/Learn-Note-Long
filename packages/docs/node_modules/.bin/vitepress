#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Desktop/learnProject/Learn-Note-Long/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.47.0_@types+node@24.10.9_axios@1.13.2_postcss@_18aeb939e0a5847db6bfbfaa3ae4c6fb/node_modules/vitepress/bin/node_modules:/Users/mac/Desktop/learnProject/Learn-Note-Long/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.47.0_@types+node@24.10.9_axios@1.13.2_postcss@_18aeb939e0a5847db6bfbfaa3ae4c6fb/node_modules/vitepress/node_modules:/Users/mac/Desktop/learnProject/Learn-Note-Long/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.47.0_@types+node@24.10.9_axios@1.13.2_postcss@_18aeb939e0a5847db6bfbfaa3ae4c6fb/node_modules:/Users/mac/Desktop/learnProject/Learn-Note-Long/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Desktop/learnProject/Learn-Note-Long/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.47.0_@types+node@24.10.9_axios@1.13.2_postcss@_18aeb939e0a5847db6bfbfaa3ae4c6fb/node_modules/vitepress/bin/node_modules:/Users/mac/Desktop/learnProject/Learn-Note-Long/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.47.0_@types+node@24.10.9_axios@1.13.2_postcss@_18aeb939e0a5847db6bfbfaa3ae4c6fb/node_modules/vitepress/node_modules:/Users/mac/Desktop/learnProject/Learn-Note-Long/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.47.0_@types+node@24.10.9_axios@1.13.2_postcss@_18aeb939e0a5847db6bfbfaa3ae4c6fb/node_modules:/Users/mac/Desktop/learnProject/Learn-Note-Long/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
